{
  "manifest_version": 2,
   "name": "Dev-Dash",
   "short_name": "Dev-Dash",
  "version": "0.0.2",
  "offline_enabled": true,

  "background": {
      "scripts": ["bundle.js"],
      "persistent": false
    },

    "chrome_url_overrides": {
      "newtab": "index.html"
    },

  "permissions": [
    "activeTab", "tabs", "storage",
    "https://ajax.googleapis.com/",
    "geolocation"
  ]
}