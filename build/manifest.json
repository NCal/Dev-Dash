{
   "manifest_version": 2,
   "name": "Dev-Dash",
   "short_name": "Dev-Dash",
   "version": "0.0.3",
   "offline_enabled": true,
   "description": "Replace a new tab page with a dashboard designed with developers and software engineers in mind.",
   "background": {
      "scripts": ["bundle.js"],
      "persistent": false
   },
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
   },
   "permissions": [
      "activeTab", "tabs", "storage",
      "https://ajax.googleapis.com/",
      "geolocation"
   ]
}